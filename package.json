{
  "name": "respammer",
  "description": "A program used to submit bogus data to phishing links",
  "author": {
    "name": "Samer Ziade",
    "url": "https://samer.io"
  },
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samerziade/respammer.git"
  },
  "main": "app.ts",
  "scripts": {
    "dev": "./node_modules/.bin/ts-node app.ts",
    "build": "./node_modules/.bin/tsc app.ts",
    "eslint:init": "./node_modules/.bin/eslint --init",
    "docker": "npm install --quiet --no-optional --no-shrinkwrap --no-package-lock && npm start --silent"
  },
  "devDependencies": {
    "@types/node": "10.5.6",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "typescript": "3.0.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "ts-node": "7.0.0"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "tabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "typescript",
    "requirePragma": false,
    "useTabs": false
  }
}
